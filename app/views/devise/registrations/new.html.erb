<div class="row">
  <div class="col-md-4">
    <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: { :multipart => true, class: "well"}) do |f| %>
      <%= devise_error_messages! %>
      <fieldset>
          <legend>Sign Up</legend>
      </fieldset> 

      <div><%= f.input :first_name %><br />

      <div><%= f.input :last_name %><br />	

      <div><%= f.input :profile_name %><br />
      	
      <div><%= f.input :email %><br />

      <div><%= f.input :password %><br />
     
      <div><%= f.input :password_confirmation %><br />

      <div><%= f.file_field :avatar %><br />

      <div><%= f.button :submit, "Sign up" %></div>
    <% end %>
  </div>
</div>    

<%= render "devise/shared/links" %>
